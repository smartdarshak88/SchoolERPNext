                                <div class="row">
                                <div class="form-group col-lg-12">
                                    <label>Location Name<span class="help"> </span></label><br>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                      
                                </div>
                                </div>
                                <div class="form-group">
                                    <label>Description<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                
                                    
                                
                                <input type="submit" class="btn btn-success float-right" value="Add Location">                                
                            </form>
                            {{/if}}
