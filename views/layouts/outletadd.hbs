                                    <input type="text" class="form-control" placeholder="George deo...">
                                      
                                </div>
                                </div>
                                <div class="form-group">
                                    <label> Full Name<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                
                                    
                                
                                <input type="submit" class="btn btn-success float-right" value="Add Weight Types">                                
