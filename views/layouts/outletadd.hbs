                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                <div class="form-group">
                                    <label> Weight Types<span class="help"></span></label>
                                    <select class="form-control select2" style='width:100%'>
                                        <option>Select Item Weight Types</option>
                                        <option>Weight Type-1</option>
                                        
                                    </select>
                                </div>
                                    
                                
