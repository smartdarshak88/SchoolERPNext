{{> header}}
{{> adminsidebar}}

<div class="page-wrapper">
        {{> breadcrums page='Add'}}
        <div class="page-content container-fluid">
            <div class="row">
                    <div class="col-sm-12">
                        <div class="card card-body">
                            <h4 class="card-title">Add {{module}}</h4>
                            <h5 class="card-subtitle"> </h5>
                            {{#if outlet}}
                            <form class="form-horizontal mt-4">
                                <div class="row">
                                <div class="form-group col-lg-12">
                                    <label> Location<span class="help"> </span></label><br>
                                    <select class="form-control select2" style='width:100%'>
                                        <option>Select Country</option>
                                        <option>India</option>
                                        <option>Australia</option>
                                        <option>New Zeland</option>
                                        <option>UK</option>
                                        <option>USA</option>
                                    </select>
                                      
                                </div>
                                </div>
                                <div class="form-group">
                                    <label> Name<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                <div class="form-group">
                                    <label> Phone<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                <div class="row">
                                <div class="form-group col-lg-12">
                                    <label> Email<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                </div>
                                <div class='row'>
                               <div class="form-group col-lg-12">
                                    <label> Description<span class="help"></span></label>
                                    <textarea type="text" class="form-control" rows=10 cols=60>
                                    </textarea>
                                </div>
                                </div>
                                <div class='row'>
                                <div class="form-group col-lg-12" >
                                    <label> Address<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                </div>
                                    
                                
                                <input type="submit" class="btn btn-success float-right" value="Add Outlet">                                
                            </form>
                            {{/if}}
                            {{#if customer}}
                            <form class="form-horizontal mt-4">
                                <div class="row">
                                <div class="form-group col-lg-12">
                                    <label> Fullname<span class="help"> </span></label><br>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                      
                                </div>
                                </div>
                                <div class="form-group">
                                    <label> Mobile<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                <div class="form-group">
                                    <label> Company Name<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                <div class="row">
                                <div class="form-group col-lg-12">
                                    <label> Person Incharge Name<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                </div>
                                <div class='row'>
                               <div class="form-group col-lg-12">
                                    <label> Photo<span class="help"></span></label>
                                    <input type="file" class="form-control" >
                                </div>
                                </div>
                                <div class='row'>
                                <div class="form-group col-lg-12" >
                                    <label> Address<span class="help"></span></label>
                                    <input type="text" class="form-control" placeholder="George deo...">
                                </div>
                                </div>
                                    
                                
                                <input type="submit" class="btn btn-success float-right" value="Add Customer">                                
                            </form>
                            {{/if}}
                            {{#if driver}}
